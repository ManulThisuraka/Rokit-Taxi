<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         <servlet>
         	<servlet-name>login</servlet-name>
         	<servlet-class>com.customer.loginservlet</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>login</servlet-name>
         	<url-pattern>/log</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>customerinsert</servlet-name>
         	<servlet-class>com.customer.customerinsert</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>customerinsert</servlet-name>
         	<url-pattern>/insert</url-pattern>
         </servlet-mapping>
         
         
         <servlet>
         	<servlet-name>bookride</servlet-name>
         	<servlet-class>com.customer.bookride</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>bookride</servlet-name>
         	<url-pattern>/bookRide</url-pattern>
         </servlet-mapping>
         
         
        
         <servlet>
         	<servlet-name>viewDetailscus</servlet-name>
         	<servlet-class>com.customer.viewuser</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>viewDetailscus</servlet-name>
         	<url-pattern>/viewdetailscus</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>updateride</servlet-name>
         	<servlet-class>com.customer.rideupdateservlet</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>updateride</servlet-name>
         	<url-pattern>/updateRide</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>updateRideDetails</servlet-name>
         	<servlet-class>com.customer.ridevalidatebyIDServlet</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>updateRideDetails</servlet-name>
         	<url-pattern>/updateRideDetails</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>deleteride</servlet-name>
         	<servlet-class>com.customer.deleteRideServlet</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>deleteride</servlet-name>
         	<url-pattern>/deleteRide</url-pattern>
         </servlet-mapping>
         
         
         
         
         
         
         
         
         
         
         
         
         

</web-app>